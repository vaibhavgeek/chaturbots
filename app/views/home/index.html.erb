    <div class="row">

<% if user_signed_in? %>
<div class="container top-margin-20">
<div class="col s3 hide-on-small-only white ">
      <ul class="sidepanel top-padding-10 ">
        <li><a href="#introduction">Introduction</a></li>
        <li><a href="#structure">Structure</a></li>
        <li><a href="#initialization">Intialization</a></li>
      </ul>
    </div>


<div class="col s3">
<div class="list-group" id="panel-show-active-members">
<% Visitor.all.each do |visitor| %>
	<% if visitor.online? %>

		<div id="<%= visitor.id %>">
			<%= link_to new_user_chat_path(:user_id => 1 , :auth_token => visitor.auth_token) , :class => "list-group-item" , :remote => "true" do %>

			<%= visitor.id %>

			<% end %>

<span class="hidden"><%= visitor.auth_token %></span>
</div>

	<% end %>
<% end %> 
</div>

</div>
<div id="display_chats" class="col s9 ">
<div class="left-margin-20">
<div class="row white ">
<div class="col s2">
              <img src="http://materializecss.com/images/yuna.jpg" alt="" class="circle responsive-img not_selected"> <!-- notice the "circle" class -->

</div>
<div class="col s2">
<img  src="http://materializecss.com/images/yuna.jpg" alt="" class="circle responsive-img selected_chat">
</div>
<div class="col s2">
<img src="http://materializecss.com/images/yuna.jpg " alt="" class="circle responsive-img not_selected">
</div>
<div class="col s2">
<img src="http://materializecss.com/images/yuna.jpg" alt="" class="circle responsive-img not_selected">
</div>

</div
</div>
	<h1> Hello World </h1>
</div>
</div>
<% else %>
<%= render ("/home/partials/home") %>

<% end %>

</div>
